{
    "editor.renderWhitespace": "all",
    "makefile.extensionOutputFolder": "./.vscode",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "files.associations": {
        "*.make": "makefile",
        "*.m++": "cpp",
        "*.cpp": "cpp",
        "*.six": "ceylon",
        "*.sax": "ceylon",
        "*.fux": "elm",
        "mathimpl.h": "c",
        "cdefs-compat.h": "c"
    }
}